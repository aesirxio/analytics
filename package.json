    "jest-environment-jsdom": "^29.4.3",
    "node-fetch": "^3.3.1",
    "prettier": "^2.8.4",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.5",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "files": [
    "dist"
